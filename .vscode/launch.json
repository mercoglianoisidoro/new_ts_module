{
    
    "version": "0.2.0",
    "configurations": [
{
    "type": "node",
    "request": "launch",
    "name": "Launch Build Program",
    "program": "${workspaceFolder}/dist/index.js"
},

 {
    "name": "index.ts",
    "type": "node",
    "request": "launch",
    "args": ["index.ts"],
    "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
    "sourceMaps": true,
    "cwd": "${workspaceRoot}",
    "protocol": "inspector"
},
{
    "name": "Current TS File",
    "type": "node",
    "request": "launch",
    "args": ["${relativeFile}"],
    "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
    "sourceMaps": true,
    "cwd": "${workspaceRoot}",
    "protocol": "inspector"
}
    ]
}